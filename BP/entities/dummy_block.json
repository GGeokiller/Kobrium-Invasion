{
  "format_version": "1.20.80",
  "minecraft:entity": {
    "description": {
      "identifier": "eu:dummy_block",
      "is_spawnable": false,
      "is_summonable": true,
      "is_experimental": false,
      "animations": {
        "dummy_falling_block": "controller.animation.dummy_falling_block"
      },
      "scripts": {
        "animate": [
          "dummy_falling_block"
        ]
      }
    },
    "component_groups": {
      "eu:despawn": {
        "minecraft:instant_despawn": {}
      }
    },
    "components": {
      "minecraft:type_family": {
        "family": [
          "armor_stand",
          "inanimate",
          "dummy"
        ]
      },
      "minecraft:scale": {
        "value": 1
      },
      //timer para despawnear la entidad
      /*       "minecraft:ageable": {
        "duration": 10,
        "grow_up": {
          "event": "eu:call_despawn"
        }
      }, */
      "minecraft:collision_box": {
        "width": 0.5,
        "height": 0.5
      },
      "minecraft:equip_item": {},
      "minecraft:health": {
        "value": 1,
        "max": 1
      },
      "minecraft:damage_sensor": {
        "triggers": {
          "cause": "all",
          "deals_damage": false
        }
      },
      "minecraft:knockback_resistance": {
        "value": 1.0
      },
      //"minecraft:nameable": {},
      "minecraft:persistent": {},
      "minecraft:physics": {
        "has_collision": true,
        "has_gravity": true
      },
      "minecraft:on_death": {
        "event": "eu:call_despawn"
      },
      "minecraft:pushable": {
        "is_pushable": false,
        "is_pushable_by_piston": true
      },
      "minecraft:conditional_bandwidth_optimization": {}
    },
    "events": {
      "eu:call_despawn": {
        "add": {
          "component_groups": [
            "eu:despawn"
          ]
        }
      },
      "eu:on_ground": {
        "add": {
          "component_groups": [
            "eu:despawn"
          ]
        }
      }
    }
  }
}