{
  "format_version": "1.21.40",
  "minecraft:entity": {
    "description": {
      "identifier": "koprium:digging_robot_projectile",
      "runtime_identifier": "minecraft:wind_charge_projectile",
      "is_spawnable": false,
      "is_summonable": true,
      "animations": {
        "destroy_ground": "controller.animation.digging_robot.destroy_ground"
      },
      "scripts": {
        "animate": [
          "destroy_ground"
        ]
      },
      "properties": {
        "eu:is_lit": {
          "type": "bool",
          "client_sync": true,
          "default": false
        }
      }
    },
    "component_groups": {
      "koprium:despawn": {
        "minecraft:instant_despawn": {}
      },
      "koprium:exploding": {
        "minecraft:explode": {
          "fuse_length": 1.5,
          "fuse_lit": true,
          "power": 3,
          "causes_fire": false,
          "destroy_affected_by_griefing": true
        }
      },
      "koprium:despawn_timer": {
        "minecraft:timer": {
          "looping": true,
          "time": 6.5,
          "time_down_event": {
            "event": "koprium:call_despawn"
          }
        }
      }
    },
    "components": {
      "minecraft:collision_box": {
        "width": 0.5,
        "height": 0.5
      },
      "minecraft:projectile": {
        "on_hit": {
          "impact_damage": {
            "damage": 12
          },
          "stick_in_ground": {
            "shake_time": 0.35
          }
        },
        "power": 1.5,
        "gravity": 0.05,
        "uncertainty_base": 7.5,
        "should_bounce": true,
        "reflect_on_hurt": true,
        "uncertainty_multiplier": 1,
        "anchor": 1,
        "destroy_on_hurt": true,
        "offset": [
          0,
          -0.1,
          1
        ],
        "inertia": 1.0,
        "liquid_inertia": 1.0
      },
      "minecraft:physics": {
        "has_collision": true
      },
      "minecraft:dimension_bound": {},
      "minecraft:pushable": {
        "is_pushable": false,
        "is_pushable_by_piston": true
      },
      "minecraft:conditional_bandwidth_optimization": {
        "default_values": {
          "max_optimized_distance": 80.0,
          "max_dropped_ticks": 7,
          "use_motion_prediction_hints": true
        }
      }
    },
    "events": {
      "minecraft:entity_spawned": {
        "add": {
          "component_groups": [
            "koprium:despawn_timer"
          ]
        }
      },
      "koprium:call_despawn": {
        "add": {
          "component_groups": [
            "koprium:despawn"
          ]
        }
      },
      "koprium:destroy_ground": {},
      "koprium:start_explosion": {
        "add": {
          "component_groups": [
            "koprium:exploding"
          ]
        },
        "remove": {
          "component_groups": [
            "koprium:despawn_timer"
          ]
        },
        "set_property": {
          "eu:is_lit": true
        }
      }
    }
  }
}