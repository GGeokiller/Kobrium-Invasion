{
	"format_version": "1.10.0",
	"animation_controllers": {
		"controller.animation.alien_cat.loop": {
			"initial_state": "alien_cat_particles",
			"states": {
				"alien_cat_particles": {
					"transitions": [
						{
							"nyan_cat_particles": "query.variant==1"
						}
					],
					"animations": [
						"particle"
					]
				},
				"nyan_cat_particles": {
					"transitions": [
						{
							"alien_cat_particles": "query.variant==0"
						}
					]
				}
			}
		},
		"controller.animation.alien_cat.dancing": {
			"initial_state": "default",
			"states": {
				"default": {
					"blend_transition": 0.2,
					"transitions": [
						{
							"dancing": "query.property('koprium:is_dancing')"
						}
					]
				},
				"dancing": {
					"blend_transition": 0.2,
					"transitions": [
						{
							"default": "!query.property('koprium:is_dancing')"
						}
					],
					"animations": [
						"dancing"
					]
				}
			}
		}
	}
}