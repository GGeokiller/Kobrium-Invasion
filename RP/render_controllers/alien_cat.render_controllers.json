{
	"format_version": "1.8.0",
	"render_controllers": {
		"controller.render.alien_cat": {
			"geometry": "Geometry.default",
			"materials": [
				{
					"*": "Material.default"
				}
			],
			"textures": [
				"query.is_name_any('Nyan Cat') ? Texture.nyan_cat : Texture.alien_cat"
			],
			"ignore_lighting": true
		}
	}
}